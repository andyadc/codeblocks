<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="routerRules" class="java.util.LinkedHashMap">
		<constructor-arg>
			<map>
				<entry value="order_workflow">
					<key>
						<map>
							<entry key="functionCode" value="10"/>
							<entry key="payMode" value="10"/>
						</map>
					</key>
				</entry>

				<entry value="acct_balance_pay_workflow">
					<key>
						<map>
							<entry key="functionCode" value="10"/>
							<entry key="payMode" value="1"/>
						</map>
					</key>
				</entry>

				<entry value="bank_card_pay_workflow">
					<key>
						<map>
							<entry key="functionCode" value="10"/>
							<entry key="payMode" value="2"/>
						</map>
					</key>
				</entry>
			</map>
		</constructor-arg>
	</bean>

</beans>
